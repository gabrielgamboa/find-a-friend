{
	"$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
		  "arrowParentheses":"always",
		  "bracketSameLine": false,
		  "bracketSpacing": true,
		  "jsxQuoteStyle": "double",
		  "quoteProperties": "asNeeded",
		  "semicolons": "always",
		  "trailingCommas": "all"
		}
	  },
	  "json": {
		"formatter": {
		  "trailingCommas": "none"
		}
	  }
}
